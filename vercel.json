{
  "version": 2,
  "name": "be-hw-2-4",
  "buildCommand": "pnpm run build",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["./node_modules/**", "src/**/*.yml"]
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "src/index.ts" }]
}
